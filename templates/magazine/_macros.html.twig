{% use 'magazine/_magazine.html.twig' %}

{%- macro magazine_inline(magazine, options) -%}
    {%- with {
        magazine: magazine,
        prefix: options.prefix ?? true,
        bolded: options.bolded ?? false
        } only -%}
        {{ block('magazine_inline') }}
    {%- endwith -%}
{%- endmacro -%}

{%- macro magazine(magazine, options) -%}
    {%- with {
        magazine: magazine,
        } only -%}
        {{ block('magazine') }}
    {%- endwith -%}
{%- endmacro -%}
