{% if user.moderatorTokens|length %}
    <div class="kbin-moderators">
        <div class="card">
            <div class="card-header">
                {{ 'topbar.moderated'|trans }}
            </div>
            <ul class="list-group list-group-flush">
                {% for moderator in user.moderatorTokens %}
                    <li class="list-group-item">
                        <span class="text-muted">/m/</span><a class="text-decoration-none stretched-link"
                                                              href="{{ path('front_magazine', {'name': moderator.magazine.name}) }}">{{ moderator.magazine.name }}</a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endif %}
